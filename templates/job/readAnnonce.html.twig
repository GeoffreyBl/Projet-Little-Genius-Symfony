{% extends 'base.html.twig' %}

{% block body %}
{{ parent()}}
<div class="container p-t150"></div>
{% for annonce in annonces %}
	<div class="row container-blocs-resultats">
				<div class="container-resultats col-12 col-sm-12 col-md-12 col-lg-12">
					<div class="card mb-5 ma-card">
						<div class="row g-0">
							<div class="col-md-8">
								<div class="card-body ma-card-body">
									<ul>
										<span>
											<small class="text-muted">{{annonce.companyname}}</small>
										</span>
										<h5 class="card-title">{{annonce.missiontitle}}</h5>
										<li class="card-text">
											<small class="text-muted">
												<i class="fas fa-briefcase"></i>&nbsp; {{annonce.type}}</small>
										</li>
										<li class="card-text">
											<small class="text-muted">
												<i class="far fa-clock"></i>&nbsp; Mise en ligne le : {{annonce.date|date("m/d/Y")}}</small>
										</li>
									</ul>
                                    <a href=""><i class="fas fa-trash"></i></a>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
            {% endfor %}
            </div>
{% endblock %}
